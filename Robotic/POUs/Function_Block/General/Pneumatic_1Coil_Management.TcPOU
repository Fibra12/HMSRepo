    <Declaration><![CDATA[FUNCTION_BLOCK Pneumatic_1Coil_Management
// Function block untuk mengolah faceplate dan watchdog pneumatic valve 1 coil
//	UDT dependencies:
	(*  
	1. stPneumatic v1.0 	*)

//	Changelog:
	(* 
	v1.0 [27.12.2021] Start development function block - Fabrilian Asali	*)	

VAR_INPUT
	bEnable						: BOOL;		
	bTriggerPneumatic			: BOOL;
	bTestMode					: BOOL;
	bResetError					: BOOL;
	bBypassSensorRetract		: BOOL;
	bBypassSensorAdvanced		: BOOL;
	bBypassPreCheckSensor		: BOOL;
	xIn_SensorRetract			: BOOL;
	xIn_SensorAdvanced			: BOOL;
	TimerBypassSensorRetract	: TIME := T#5S;
	TimerBypassSensorAdvanced	: TIME := T#5S;
	TimerWatchdogRetract		: TIME := T#5S;
	TimerWatchdogAdvanced		: TIME := T#5S;
	TimerWatchdogPreRetract		: TIME := T#5S;
	TimerWatchdogPreAdvanced	: TIME := T#5S;
END_VAR

VAR_OUTPUT
	bError						: BOOL;
	ErrorID						: INT;
	ErrorMsg					: STRING;
END_VAR

VAR_IN_OUT
	Pneumatic					: stPneumatic1;
END_VAR

VAR
	SeqIndex					: INT;
	WatchdogTimer				: TON;
	bTrig_Output				: BOOL;
	PneuAdv						: R_TRIG;
	TimerBypass					: TON;
END_VAR]]></Declaration>
