    <Declaration><![CDATA[FUNCTION_BLOCK Pneumatic_2Coil_Management //V1.0
// Function block untuk mengolah faceplate dan watchdog pneumatic valve 2 coil
//	UDT dependencies:
	(*  
	1. stPneumatic2 v1.0 	*)

//	Changelog:
	(* 
	
	v1.0 [27.12.2021] Start development function block - Fabrilian Asali	
	v1.1 [24.05.2023] Add Pneumatic Name - Arif W*)	
	

VAR_INPUT
	bEnable						: BOOL;		
	bTriggerPneumaticRetract	: BOOL;
	bTriggerPneumaticAdvanced	: BOOL;
	bTriggerBoth				: BOOL;
	bReleaseBoth				: BOOL;
	bTestMode					: BOOL;
	bResetError					: BOOL;
	bBypassSensorRetract		: BOOL;
	bBypassSensorAdvanced		: BOOL;
	bBypassPreCheckSensor		: BOOL;
	xIn_SensorRetract			: BOOL;
	xIn_SensorAdvanced			: BOOL;
	TimerWatchdogRetract		: TIME := T#5S;
	TimerWatchdogAdvanced		: TIME := T#5S;
	TimerWatchdogPreRetract		: TIME := T#5S;
	TimerWatchdogPreAdvanced	: TIME := T#5S;
	TimerValve					: TIME := T#25MS;
	Name						: STRING;
END_VAR

VAR_OUTPUT
	bError						: BOOL;
	ErrorID						: INT;
	ErrorMsg					: STRING;
END_VAR

VAR_IN_OUT
	Pneumatic					: stPneumatic2;
END_VAR

VAR
	SeqIndex					: INT;
	SeqIndexAdv					: INT;
	SeqIndexRtc					: INT;
	InitSeqIndex				: INT;
	WatchdogTimer				: TON;
	ValveTimer					: TON;
	ValveTimerInit				: TON;
	bInit						: BOOL;
	PneuAdv						: R_TRIG;
	PneuRtc						: R_TRIG;
END_VAR
]]></Declaration>
