    <Action Name="Cycle" Id="{0ac73b5f-61d9-4637-b8e3-ecc3ec4493f1}">
      <Implementation>
        <ST><![CDATA[// Pneumatic Advanced Cycle Counter
PneuAdv(CLK := Pneumatic.oPneumaticAdvanced AND Pneumatic.iSensorAdvanced); // Pemicu untuk menghitung siklus pneumatik maju dengan menghubungkan sinyal pengontrol dan sensor maju
IF PneuAdv.Q THEN // Jika siklus pneumatik maju terdeteksi
	Pneumatic.CyclePneuAdv := Pneumatic.CyclePneuAdv + 1; // Tambahkan satu ke hitungan siklus pneumatik maju
END_IF

// Pneumatic Retract Cycle Counter
PneuRtc(CLK := Pneumatic.oPneumaticRetract AND Pneumatic.iSensorRetract); // Pemicu untuk menghitung siklus pneumatik mundur dengan menghubungkan sinyal pengontrol dan sensor mundur
IF PneuRtc.Q THEN // Jika siklus pneumatik mundur terdeteksi
	Pneumatic.CyclePneuRtc := Pneumatic.CyclePneuRtc + 1; // Tambahkan satu ke hitungan siklus pneumatik mundur
END_IF

(*PneuAdv(CLK := Pneumatic.Trigger_Advanced);
CASE SeqIndexAdv OF
	0:
		IF PneuAdv.Q THEN
			Pneumatic.CyclePneuAdv	:= Pneumatic.CyclePneuAdv + 1;
			SeqIndexAdv				:= 1;
		END_IF
		
	1:
		IF NOT PneuAdv.Q THEN
			SeqIndexAdv				:= 0;
		END_IF
END_CASE

PneuRtc(CLK := Pneumatic.Trigger_Retract);
CASE SeqIndexRtc OF
	0:
		IF PneuRtc.Q THEN
			Pneumatic.CyclePneuRtc	:= Pneumatic.CyclePneuRtc + 1;
			SeqIndexRtc				:= 1;
		END_IF
		
	1:
		IF NOT PneuRtc.Q THEN
			SeqIndexRtc				:= 0;
		END_IF
END_CASE*)]]></ST>
      </Implementation>
    </Action>
    <Action Name="Init" Id="{8d03c11a-dad1-4883-8630-a4dad8d8fdce}">
      <Implementation>
        <ST><![CDATA[// Reset Error Handling
IF bResetError THEN // Jika reset kesalahan diaktifkan
	bError := FALSE; // Set variabel bError ke FALSE untuk menunjukkan tidak ada kesalahan
	InitSeqIndex := 0; // Set indeks urutan inisialisasi kembali ke 0
END_IF

// Inisialisasi Pneumatic
IF bEnable AND NOT bInit THEN // Jika sistem diaktifkan dan belum diinisialisasi
	CASE InitSeqIndex OF // Berdasarkan indeks urutan inisialisasi
		0:
			IF xIn_SensorRetract THEN // Jika sensor retract aktif
				InitSeqIndex := 100; // Pergi ke langkah 100
			ELSIF xin_SensorAdvanced THEN // Jika sensor advanced aktif
				InitSeqIndex := 200; // Pergi ke langkah 200
			ELSIF NOT xIn_SensorRetract AND NOT xIn_SensorAdvanced THEN // Jika kedua sensor tidak aktif
				InitSeqIndex := 300; // Pergi ke langkah 300
			ELSE // Jika kedua sensor aktif pada saat inisialisasi
				ErrorID := 1; // Set ID kesalahan ke 1
				ErrorMsg := 'Both Sensor on at Init'; // Pesan kesalahan
				InitSeqIndex := 999; // Pergi ke langkah 999
			END_IF
	
		100:
			Pneumatic.oPneumaticRetract := TRUE; // Aktifkan pneumatik retract
			InitSeqIndex := 1000; // Pergi ke langkah 1000
		
		200:
			Pneumatic.oPneumaticAdvanced := TRUE; // Aktifkan pneumatik advanced
			InitSeqIndex := 1000; // Pergi ke langkah 1000
		
		300:
			Pneumatic.oPneumaticRetract := TRUE; // Aktifkan pneumatik retract
			Pneumatic.oPneumaticAdvanced := TRUE; // Aktifkan pneumatik advanced
			ValveTimerInit(In := TRUE, PT := TimerValve); // Aktifkan timer untuk inisialisasi katup
			IF ValveTimerInit.Q THEN // Jika timer katup aktif
			    ValveTimerInit(In := FALSE); // Nonaktifkan timer katup
				Pneumatic.oPneumaticAdvanced := FALSE; // Nonaktifkan pneumatik advanced
				Pneumatic.oPneumaticRetract := FALSE; // Nonaktifkan pneumatik retract
				InitSeqIndex := 1000; // Pergi ke langkah 1000
			END_IF;
		
		999:
			bError := TRUE; // Set variabel bError ke TRUE untuk menunjukkan kesalahan
		
		1000:
			bInit := TRUE; // Set variabel bInit ke TRUE untuk menunjukkan inisialisasi selesai
	END_CASE
END_IF
]]></ST>
      </Implementation>
    </Action>
    <Action Name="ValveManagement" Id="{4e1eb282-b367-4946-8fce-1656a6a057ae}">
      <Implementation>
        <ST><![CDATA[// Reset Error Handling
IF bResetError THEN // Jika reset kesalahan diaktifkan
	bError := FALSE; // Set variabel bError ke FALSE untuk menunjukkan tidak ada kesalahan
	ErrorID := 0; // Set ID kesalahan kembali ke 0
	ErrorMsg := ''; // Kosongkan pesan kesalahan
	SeqIndex := 0; // Set indeks urutan kembali ke 0
END_IF

// Kontrol Pneumatik
IF bEnable AND bInit AND NOT bError THEN // Jika sistem diaktifkan, sudah diinisialisasi, dan tidak ada kesalahan
	CASE SeqIndex OF // Berdasarkan indeks urutan
		0: // Langkah awal
			IF Pneumatic.Trigger_Retract THEN // Jika triger retract aktif
				SeqIndex := 100; // Pergi ke langkah 100
			ELSIF Pneumatic.Trigger_Advanced THEN // Jika triger advanced aktif
				SeqIndex := 200; // Pergi ke langkah 200
			END_IF
			
		100: // Langkah untuk retract
			Pneumatic.Trigger_Retract := FALSE; // Nonaktifkan triger retract
			IF NOT bBypassPreCheckSensor THEN // Jika tidak melewati pengecekan sensor pra-retract
				WatchdogTimer(In := TRUE, PT := TimerWatchdogPreRetract); // Aktifkan timer pengawas pra-retract
				IF Pneumatic.iSensorAdvanced THEN // Jika sensor advanced aktif
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					SeqIndex := 105; // Pergi ke langkah 105
				ELSIF WatchdogTimer.Q THEN // Jika timer habis
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					Pneumatic.Error_Retract := TRUE; // Set kesalahan retract
					ErrorID := 10; // Set ID kesalahan
					ErrorMsg := CONCAT(Name, 'Error PreCheck Retract'); // Set pesan kesalahan
					SeqIndex := 999; // Pergi ke langkah 999
				END_IF
			ELSE
				SeqIndex := 105; // Pergi ke langkah 105
			END_IF
	
		105: // Langkah setelah pra-retract
			Pneumatic.oPneumaticRetract := TRUE; // Aktifkan pneumatik retract
			Pneumatic.oPneumaticAdvanced := TRUE; // Aktifkan pneumatik advanced
			ValveTimer(In := TRUE, PT := TimerValve); // Aktifkan timer katup
			IF ValveTimer.Q THEN // Jika timer katup aktif
				ValveTimer(In := FALSE); // Nonaktifkan timer
				Pneumatic.oPneumaticAdvanced := FALSE; // Nonaktifkan pneumatik advanced
				SeqIndex := 110; // Pergi ke langkah 110
			END_IF
			
		110: // Langkah setelah retract
			IF NOT bBypassSensorRetract THEN // Jika tidak melewati sensor retract
				WatchdogTimer(In := TRUE, PT := TimerWatchdogRetract); // Aktifkan timer pengawas retract
				IF Pneumatic.iSensorRetract THEN // Jika sensor retract aktif
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					SeqIndex := 0; // Kembali ke langkah awal
				ELSIF WatchdogTimer.Q THEN // Jika timer habis
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					Pneumatic.Error_Retract := TRUE; // Set kesalahan retract
					ErrorID := 11; // Set ID kesalahan
					ErrorMsg := CONCAT(Name, 'Error Watchdog Retract'); // Set pesan kesalahan
				END_IF
			ELSE
				SeqIndex := 0; // Kembali ke langkah awal
			END_IF
		
		200: // Langkah untuk advanced
			Pneumatic.Trigger_Advanced := FALSE; // Nonaktifkan triger advanced
			IF NOT bBypassPreCheckSensor THEN // Jika tidak melewati pengecekan sensor pra-advanced
				WatchdogTimer(In := TRUE, PT := TimerWatchdogPreAdvanced); // Aktifkan timer pengawas pra-advanced
				IF Pneumatic.iSensorRetract THEN // Jika sensor retract aktif
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					SeqIndex := 210; // Pergi ke langkah 210
				ELSIF WatchdogTimer.Q THEN // Jika timer habis
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					Pneumatic.Error_Advanced := TRUE; // Set kesalahan advanced
					ErrorID := 20; // Set ID kesalahan
					ErrorMsg := CONCAT(Name, 'Error PreCheck Advanced'); // Set pesan kesalahan
					SeqIndex := 999; // Pergi ke langkah 999
				END_IF
			ELSE
				SeqIndex := 205; // Pergi ke langkah 205
			END_IF				
		
		205: // Langkah setelah pra-advanced
			Pneumatic.oPneumaticRetract := TRUE; // Aktifkan pneumatik retract
			Pneumatic.oPneumaticAdvanced := TRUE; // Aktifkan pneumatik advanced
			ValveTimer(In := TRUE, PT := TimerValve); // Aktifkan timer katup
			IF ValveTimer.Q THEN // Jika timer katup aktif
				Pneumatic.oPneumaticRetract := FALSE; // Nonaktifkan pneumatik retract
				SeqIndex := 210; // Pergi ke langkah 210
			END_IF	
			
		210: // Langkah setelah advanced
			IF NOT bBypassSensorAdvanced THEN // Jika tidak melewati sensor advanced
				WatchdogTimer(In := TRUE, PT := TimerWatchdogAdvanced); // Aktifkan timer pengawas advanced
				IF Pneumatic.iSensorAdvanced THEN // Jika sensor advanced aktif
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					SeqIndex := 0; // Kembali ke langkah awal
				ELSIF WatchdogTimer.Q THEN // Jika timer habis
					WatchdogTimer(In := FALSE); // Nonaktifkan timer
					Pneumatic.Error_Advanced := TRUE; // Set kesalahan advanced
					ErrorID := 21; // Set ID kesalahan
					ErrorMsg := CONCAT(Name, 'Error Watchdog Advanced'); // Set pesan kesalahan
				END_IF
				ELSE
				SeqIndex	:= 0; // Kembali ke langkah awal
			END_IF					
	
		999: // Langkah jika ada kesalahan
			bError := TRUE; // Set variabel bError ke TRUE untuk menunjukkan ada kesalahan	
	
	END_CASE
	
ELSE // Jika tidak ada aktivitas
	SeqIndex := 0; // Set indeks urutan kembali ke 0
	WatchdogTimer(IN := FALSE); // Nonaktifkan timer pengawas
END_IF
]]></ST>
      </Implementation>
    </Action>
  </POU>
